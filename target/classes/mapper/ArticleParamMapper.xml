<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.javabull.ssm.blog.dao.ArticleParamMapper">

    <select id="selectContentByArticleId" resultType="java.lang.String">
        select article_content from tb_article_full_content
        where tb_article_full_content.article_id = #{articleId}
    </select>
</mapper>